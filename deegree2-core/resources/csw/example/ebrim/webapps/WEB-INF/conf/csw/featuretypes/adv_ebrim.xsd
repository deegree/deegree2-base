<xsd:schema targetNamespace="http://www.deegree.org/app" xmlns:gml="http://www.opengis.net/gml"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:deegreewfs="http://www.deegree.org/wfs"
  xmlns:ogc="http://www.opengis.net/ogc" xmlns:app="http://www.deegree.org/app" xmlns:wfs="http://www.opengis.net/wfs"
  elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xsd:import namespace="http://www.opengis.net/gml"
    schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/feature.xsd" />
  <xsd:import namespace="http://www.opengis.net/wfs" schemaLocation="http://schemas.opengis.net/wfs/1.1.0/wfs.xsd" />

  <!-- configuration for the persistence backend to be used -->
  <xsd:annotation>
    <xsd:appinfo>
      <deegreewfs:Prefix>app</deegreewfs:Prefix>
      <deegreewfs:Backend>POSTGIS</deegreewfs:Backend>
      <deegreewfs:DefaultSRS>EPSG:4326</deegreewfs:DefaultSRS>
      <JDBCConnection xmlns="http://www.deegree.org/jdbc">
        <Driver>org.postgresql.Driver</Driver>
        <Url>jdbc:postgresql://hurricane:5432/adv_registry_intern</Url>
        <!--Url>jdbc:postgresql://hurricane:5432/adv_registry</Url-->
        <User>adv</User>
        <Password>adv</Password>
        <SecurityConstraints />
        <Encoding>UTF-8</Encoding>
      </JDBCConnection>
      <deegreewfs:SuppressXLinkOutput>true</deegreewfs:SuppressXLinkOutput>
    </xsd:appinfo>
  </xsd:annotation>
  <!-- ============================================================== -->
  <xsd:element name="RegistryObject" type="app:RegistryObjectType" substitutionGroup="gml:_Feature">
    <xsd:annotation>
      <xsd:appinfo>
        <deegreewfs:table>RegistryObject</deegreewfs:table>
        <deegreewfs:gmlId prefix="RO_">
          <deegreewfs:MappingField field="id" type="NUMERIC" />
          <deegreewfs:IdGenerator type="DB_SEQ">
            <deegreewfs:param name="sequence">RegistryObject_ID_seq</deegreewfs:param>
          </deegreewfs:IdGenerator>
        </deegreewfs:gmlId>
        <deegreewfs:visible>true</deegreewfs:visible>
        <deegreewfs:transaction update="true" delete="true" insert="true" />
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <!-- ============================================================== -->
  <xsd:complexType name="RegistryObjectType">
    <xsd:complexContent>
      <xsd:extension base="gml:AbstractFeatureType">
        <xsd:sequence>
          <xsd:element name="linkedRegistryObject" type="gml:FeaturePropertyType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart> 
                <deegreewfs:Content type="app:LINK_RegObj_RegObj">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_source" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="name" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
                <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart> 
                <deegreewfs:Content type="app:Name">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="description" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:Description">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="slots" type="gml:FeaturePropertyType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:Slot">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobj" type="NUMERIC" table="JT_SLOT_RegistryObj" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                  <deegreewfs:Relation>
                    <deegreewfs:From fk="true">
                      <deegreewfs:MappingField field="fk_slot" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="type" type="xsd:string" minOccurs="0">
            <xsd:annotation>
                          <xsd:appinfo>
                                     <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart> 
                <deegreewfs:Content>
                  <deegreewfs:MappingField field="type" type="VARCHAR" />
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="iduri" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo>
                <deegreewfs:Content>
                  <deegreewfs:MappingField field="iduri" type="VARCHAR" />
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="home" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content>
                  <deegreewfs:MappingField field="home" type="VARCHAR" />
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="liduri" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
                <deegreewfs:Content>
                  <deegreewfs:MappingField field="liduri" type="VARCHAR" />
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="objectType" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content>
                  <deegreewfs:MappingField field="objecttype" type="VARCHAR" />
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="status" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content>
                  <deegreewfs:MappingField field="status" type="VARCHAR" />
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="versionInfo" type="gml:FeaturePropertyType" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:VersionInfo">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="association" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:Association">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="auditableEvent" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:AuditableEvent">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="classification" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:Classification">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="classificationNode" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:ClassificationNode">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="classificationScheme" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:ClassificationScheme">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="externalIdentifier" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:ExternalIdentifier">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="extrinsicObject" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:ExtrinsicObject">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="registryPackage" type="gml:FeaturePropertyType" minOccurs="0">
            <xsd:annotation>
              <xsd:appinfo>
              <deegreewfs:IdentityPart>false</deegreewfs:IdentityPart>               
                <deegreewfs:Content type="app:RegistryPackage">
                  <deegreewfs:Relation>
                    <deegreewfs:From>
                      <deegreewfs:MappingField field="ID" type="NUMERIC" />
                    </deegreewfs:From>
                    <deegreewfs:To fk="true">
                      <deegreewfs:MappingField field="fk_registryobject" type="NUMERIC" />
                    </deegreewfs:To>
                  </deegreewfs:Relation>
                </deegreewfs:Content>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ============================================================== -->
  <xsd:element name="Association" type="app:AssociationType" substitutionGroup="gml:_Feature">
    <xsd:annotation>
      <xsd:appinfo>
        <deegreewfs:table>Association</deegreewfs:table>
        <deegreewfs:gmlId prefix="ASS_">
          <deegreewfs:MappingField field="id" type="NUMERIC" />
          <deegreewfs:IdGenerator type="DB_SEQ">
            <deegreewfs:param name="sequence">Association_ID_seq</deegreewfs:param>
          </deegreewfs:IdGenerator>
        </deegreewfs:gmlId>
        <deegreewfs:visible>false</deegreewfs:visible>
        <deegreewfs:transaction update="true" delete="true" insert="true" />
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:element>
  <!-- ============================================================== -->
  <xsd:complexType name="AssociationType">
    <xsd:complexContent>
      <xsd:extension base="gml:AbstractFeatureType">
        <xsd:sequence>
          <xsd:element name="associationType" type="xsd:string">
            <xsd:annotation>
              <xsd:appinfo>
                <deegreewfs:Content>
